{
  "color": {
    "dark": {
      "on media": "#ffffff",
      "error": "#b3261e",
      "surface": {
        "dark": "#181818",
        "light": "#353535",
        "medium": {
          "enabled": "#222222",
          "disabled": "#0e0e0e"
        }
      },
      "on surface": {
        "dark": "#5d5d5d",
        "medium": "#8b8b8b",
        "light": {
          "enabled": "#e4e4e4",
          "disabled": "#5b5b5b"
        }
      }
    },
    "*opacity": {
      "text": {
        "medium": "#ffffff80",
        "light": "#ffffff4d"
      },
      "on media": {
        "medium": "#18181899",
        "light": "#18181833"
      }
    }
  },
  "gradient": {
    "*gradient": {
      "top": {
        "medium": {
          "gradientType": "linear",
          "rotation": 180,
          "stops": [
            {
              "position": 0,
              "color": "#181818ff"
            },
            {
              "position": 1,
              "color": "#18181833"
            }
          ]
        },
        "light": {
          "gradientType": "linear",
          "rotation": 180,
          "stops": [
            {
              "position": 0,
              "color": "#181818cc"
            },
            {
              "position": 1,
              "color": "#18181833"
            }
          ]
        }
      },
      "bottom": {
        "medium": {
          "gradientType": "linear",
          "rotation": 180,
          "stops": [
            {
              "position": 0,
              "color": "#18181800"
            },
            {
              "position": 1,
              "color": "#181818ff"
            }
          ]
        },
        "light": {
          "gradientType": "linear",
          "rotation": 180,
          "stops": [
            {
              "position": 0,
              "color": "#18181800"
            },
            {
              "position": 1,
              "color": "#181818cc"
            }
          ]
        }
      }
    }
  },
